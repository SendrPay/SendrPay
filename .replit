modules = ["nodejs-20", "postgresql-16"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"

[workflows]

[[ports]]
localPort = 5000
externalPort = 80

[deployment]
deploymentTarget = "gce"
run = ["sh", "-c", "npx prisma generate && npx prisma db push --skip-generate && npx tsx src/index.ts"]
